[color]
	ui = true
[core]
	editor = emacs -nw
	excludesfile = ~/.gitignore
[credential]
	helper = store
[push]
	default = current
[user]
	name = Jeff Coffler
	email = jeff@taltos.com
[help]
	autoCorrect = -1
[log]
	abbrevCommit = true
	decorate = short
	date = local
[apply]
	ignoreWhitespace = change
[am]
	threeWay = true
[rerere]
	enabled = true
	autoUpdate = true
[alias]
	lol = log --oneline --graph
	nuke = !git clean -fdx && git submodule foreach git clean -fdx
	sdevelop = !git submodule foreach git checkout develop
	sup = !git submodule foreach git pull
	sync-branch = remote prune origin
	sync-subs = !git submodule foreach git pull
	rmrbranch = push origin --delete  # <Branch-name> supplied by user
